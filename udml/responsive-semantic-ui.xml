<SCREEN>
  <AppLayout display="flex" flexDirection="row">
    <!-- Left Sidebar Navigation -->
    <Sidebar width="240px" flexShrink="0" flexGrow="0" fillColor="#ffffff" strokeColor="#000000" strokeOpacity="0.1">
      <!-- Sidebar Header with Logo and User Menu -->
      <SidebarHeader padding="10px 10px 10px 10px" display="flex" justifyContent="space-between">
        <LogoArea display="flex" alignItems="center">
          <Logo padding="0" display="flex" alignItems="center" fillColor="#ffffff" fillOpacity="0">
            <LogoIcon width="24px" height="24px" fillColor="#abadaf" />
            <LogoText textStyle="text-sm/lineHeight-5/font-semibold">Saas UI</LogoText>
          </Logo>
        </LogoArea>
        <UserMenu display="flex" justifyContent="flex-end" alignItems="center">
          <NotificationIcon padding="8px 6px" fillColor="#ffffff" fillOpacity="0">
            <Badge width="6px" height="6px" fillColor="#0ea371" strokeColor="#ffffff" />
          </NotificationIcon>
        </UserMenu>
      </SidebarHeader>

      <!-- Navigation Menu -->
      <NavigationMenu padding="0 10px 0 10px" display="flex" flexDirection="column" flexGrow="1" gap="10px">
        <!-- Primary Navigation -->
        <PrimaryNavigation display="flex" flexDirection="column" gap="1px">
          <NavItem padding="0 16px" height="32px" display="flex" alignItems="center" gap="14px" fillColor="#e7e7e8" isActive="true">
            <NavIcon width="14px" height="14px" />
            <NavText textStyle="component-text/sm">Dashboard</NavText>
          </NavItem>
          <NavItem padding="0 16px" height="32px" display="flex" alignItems="center" gap="14px">
            <NavIcon width="14px" height="14px" />
            <NavText textStyle="component-text/sm">Inbox</NavText>
          </NavItem>
          <NavItem padding="0 16px" height="32px" display="flex" alignItems="center" gap="14px">
            <NavIcon width="14px" height="14px" />
            <NavText textStyle="component-text/sm">Contacts</NavText>
          </NavItem>
        </PrimaryNavigation>

        <!-- Tags Section -->
        <TagsNavigation display="flex" flexDirection="column" gap="1px">
          <SectionHeading padding="0 8px" height="24px" display="flex" justifyContent="space-between" alignItems="center">
            <SectionTitle textStyle="component-text/xs">Tags</SectionTitle>
            <CollapseIcon width="12px" height="12px" />
          </SectionHeading>
          <TagItem padding="0 16px" height="32px" display="flex" alignItems="center" gap="14px">
            <TagIcon width="14px" height="14px" fillColor="#8952e0" />
            <TagText textStyle="component-text/sm">Developer</TagText>
            <TagCount marginLeft="auto" textStyle="text-sm/lineHeight-5/font-normal">210</TagCount>
          </TagItem>
          <TagItem padding="0 16px" height="32px" display="flex" alignItems="center" gap="14px">
            <TagIcon width="14px" height="14px" fillColor="#0ea371" />
            <TagText textStyle="component-text/sm">Designer</TagText>
            <TagCount marginLeft="auto" textStyle="text-sm/lineHeight-5/font-normal">83</TagCount>
          </TagItem>
          <TagItem padding="0 16px" height="32px" display="flex" alignItems="center" gap="14px">
            <TagIcon width="14px" height="14px" fillColor="#347fdb" />
            <TagText textStyle="component-text/sm">Partner</TagText>
            <TagCount marginLeft="auto" textStyle="text-sm/lineHeight-5/font-normal">12</TagCount>
          </TagItem>
          <TagItem padding="0 16px" height="32px" display="flex" alignItems="center" gap="14px">
            <TagIcon width="14px" height="14px" fillColor="#7d7f83" />
            <TagText textStyle="component-text/sm">Prospect</TagText>
            <TagCount marginLeft="auto" textStyle="text-sm/lineHeight-5/font-normal">23</TagCount>
          </TagItem>
        </TagsNavigation>
      </NavigationMenu>

      <!-- Sidebar Footer -->
      <SidebarFooter padding="10px 10px 10px 10px" display="flex" alignItems="center">
        <SettingsIcon padding="8px 6px" />
      </SidebarFooter>
    </Sidebar>

    <!-- Main Content Area -->
    <MainContent flexGrow="1" display="flex" flexDirection="column">
      <!-- Top Navigation Bar -->
      <TopNavbar padding="10px 16px" height="52px" display="flex" justifyContent="space-between" alignItems="center" fillColor="#ffffff" strokeColor="#000000" strokeOpacity="0.1">
        <PageTitle textStyle="text-sm/lineHeight-5/font-semibold">Dashboard</PageTitle>
        <ActionButtons display="flex" gap="10px">
          <Button padding="6px 12px" height="32px" fillColor="#8952e0" strokeColor="#8952e0" textStyle="text-sm/lineHeight-5/font-semibold">Buy Pro</Button>
        </ActionButtons>
      </TopNavbar>

      <!-- Filter Bar -->
      <FilterBar padding="10px 16px" height="52px" display="flex" alignItems="center" gap="12px" fillColor="#ffffff" strokeColor="#000000" strokeOpacity="0.1">
        <DateFilterGroup display="flex" height="32px" strokeColor="#e7e7e8">
          <DateOption padding="6px 12px" textStyle="text-sm/lineHeight-5/font-semibold">1 day</DateOption>
          <Divider strokeColor="#e7e7e8" />
          <DateOption padding="6px 12px" fillColor="#e7e7e8" isActive="true" textStyle="text-sm/lineHeight-5/font-semibold">3 days</DateOption>
          <Divider strokeColor="#e7e7e8" />
          <DateOption padding="6px 12px" textStyle="text-sm/lineHeight-5/font-semibold">1 week</DateOption>
          <Divider strokeColor="#e7e7e8" />
          <DateOption padding="6px 12px" textStyle="text-sm/lineHeight-5/font-semibold">1 month</DateOption>
          <Divider strokeColor="#e7e7e8" />
          <DateOption padding="6px 12px" textStyle="text-sm/lineHeight-5/font-semibold">Custom</DateOption>
        </DateFilterGroup>
        <DateRangePicker padding="4px 12px" height="32px" display="flex" alignItems="center" gap="10px" strokeColor="#e7e7e8">
          <CalendarIcon width="14px" height="14px" />
          <DateRangeText textStyle="text-sm/lineHeight-5/font-semibold">12/20/2023 - 1/19/2024</DateRangeText>
        </DateRangePicker>
      </FilterBar>

      <!-- Dashboard Content -->
      <DashboardContent padding="24px 10px" flexGrow="1" display="flex" flexDirection="column" gap="28px">
        <!-- Metrics and Chart Card -->
        <MetricsCard padding="0 0 32px 0" display="flex" flexDirection="column" gap="10px" fillColor="#ffffff" strokeColor="#e7e7e8">
          <!-- KPI Metrics Row -->
          <MetricsRow display="flex" width="100%">
            <!-- Revenue Metric -->
            <MetricItem flex="1" padding="20px 20px" display="flex" flexDirection="column">
              <MetricContent display="flex" flexDirection="column" gap="4px">
                <MetricLabel textStyle="text-sm/lineHeight-5/font-medium">Revenue</MetricLabel>
                <MetricValueRow display="flex" alignItems="center" gap="10px">
                  <MetricValue textStyle="text-3xl/lineHeight-9/font-semibold">€39,623</MetricValue>
                  <MetricChange direction="up" display="flex" alignItems="center" gap="4px">
                    <ArrowUpIcon width="15px" height="15px" fillColor="#0ea371" />
                    <ChangeText textStyle="Inter Regular 13pt" color="#0ea371">8%</ChangeText>
                  </MetricChange>
                </MetricValueRow>
                <MetricComparison textStyle="Inter Regular 13pt">vs. €34.234 last period</MetricComparison>
              </MetricContent>
            </MetricItem>
            
            <!-- New Customers Metric -->
            <MetricItem flex="1" padding="20px 20px" display="flex" flexDirection="column" strokeColor="#e7e7e8">
              <MetricContent display="flex" flexDirection="column" gap="4px">
                <MetricLabel textStyle="text-sm/lineHeight-5/font-medium">New customers</MetricLabel>
                <MetricValueRow display="flex" alignItems="center" gap="10px">
                  <MetricValue textStyle="text-3xl/lineHeight-9/font-semibold">130</MetricValue>
                  <MetricChange direction="down" display="flex" alignItems="center" gap="4px">
                    <ArrowDownIcon width="15px" height="15px" fillColor="#e53e3e" />
                    <ChangeText textStyle="Inter Regular 13pt" color="#e53e3e">8%</ChangeText>
                  </MetricChange>
                </MetricValueRow>
                <MetricComparison textStyle="Inter Regular 13pt">vs. 34.234 last period</MetricComparison>
              </MetricContent>
            </MetricItem>
            
            <!-- Churned Customers Metric -->
            <MetricItem flex="1" padding="20px 20px" display="flex" flexDirection="column" strokeColor="#e7e7e8">
              <MetricContent display="flex" flexDirection="column" gap="4px">
                <MetricLabel textStyle="text-sm/lineHeight-5/font-medium">Churned customers</MetricLabel>
                <MetricValueRow display="flex" alignItems="center" gap="10px">
                  <MetricValue textStyle="text-3xl/lineHeight-9/font-semibold">7</MetricValue>
                  <MetricChange direction="down" display="flex" alignItems="center" gap="4px">
                    <ArrowDownIcon width="15px" height="15px" fillColor="#e53e3e" />
                    <ChangeText textStyle="Inter Regular 13pt" color="#e53e3e">8%</ChangeText>
                  </MetricChange>
                </MetricValueRow>
                <MetricComparison textStyle="Inter Regular 13pt">vs. 3 last period</MetricComparison>
              </MetricContent>
            </MetricItem>
            
            <!-- Active Customers Metric -->
            <MetricItem flex="1" padding="20px 20px" display="flex" flexDirection="column" strokeColor="#e7e7e8">
              <MetricContent display="flex" flexDirection="column" gap="4px">
                <MetricLabel textStyle="text-sm/lineHeight-5/font-medium">Active customers</MetricLabel>
                <MetricValueRow display="flex" alignItems="center" gap="10px">
                  <MetricValue textStyle="text-3xl/lineHeight-9/font-semibold">1199</MetricValue>
                  <MetricChange direction="up" display="flex" alignItems="center" gap="4px">
                    <ArrowUpIcon width="15px" height="15px" fillColor="#0ea371" />
                    <ChangeText textStyle="Inter Regular 13pt" color="#0ea371">8%</ChangeText>
                  </MetricChange>
                </MetricValueRow>
                <MetricComparison textStyle="Inter Regular 13pt">vs. 1199 last period</MetricComparison>
              </MetricContent>
            </MetricItem>
          </MetricsRow>
          
          <!-- Chart Area -->
          <ChartArea padding="32px 0 0 0" display="flex" gap="10px">
            <YAxisLabels display="flex" flexDirection="column" justifyContent="space-between" alignItems="flex-end">
              <YAxisLabel textStyle="Inter Regular 12pt">€3,600</YAxisLabel>
              <YAxisLabel textStyle="Inter Regular 12pt">€2,900</YAxisLabel>
              <YAxisLabel textStyle="Inter Regular 12pt">€1,900</YAxisLabel>
              <YAxisLabel textStyle="Inter Regular 12pt">€900</YAxisLabel>
              <YAxisLabel textStyle="Inter Regular 12pt">0€</YAxisLabel>
            </YAxisLabels>
            
            <Chart flexGrow="1" position="relative">
              <!-- X-Axis Labels -->
              <XAxisLabels display="flex" justifyContent="space-between" width="100%">
                <XAxisLabel textStyle="Inter Regular 12pt">7/8</XAxisLabel>
                <XAxisLabel textStyle="Inter Regular 12pt">8/8</XAxisLabel>
                <XAxisLabel textStyle="Inter Regular 12pt">9/8</XAxisLabel>
                <XAxisLabel textStyle="Inter Regular 12pt">10/8</XAxisLabel>
                <XAxisLabel textStyle="Inter Regular 12pt">11/8</XAxisLabel>
                <XAxisLabel textStyle="Inter Regular 12pt">12/8</XAxisLabel>
                <XAxisLabel textStyle="Inter Regular 12pt">13/8</XAxisLabel>
                <XAxisLabel textStyle="Inter Regular 12pt">14/8</XAxisLabel>
                <XAxisLabel textStyle="Inter Regular 12pt">15/8</XAxisLabel>
                <XAxisLabel textStyle="Inter Regular 12pt">16/8</XAxisLabel>
                <XAxisLabel textStyle="Inter Regular 12pt">17/8</XAxisLabel>
                <XAxisLabel textStyle="Inter Regular 12pt">18/8</XAxisLabel>
                <XAxisLabel textStyle="Inter Regular 12pt">19/8</XAxisLabel>
                <XAxisLabel textStyle="Inter Regular 12pt">20/8</XAxisLabel>
                <XAxisLabel textStyle="Inter Regular 12pt">21/8</XAxisLabel>
              </XAxisLabels>
              
              <!-- Grid Lines -->
              <GridLines display="flex" flexDirection="column" justifyContent="space-between" height="100%" position="absolute" width="100%">
                <HorizontalLine strokeColor="#d3d4d5" strokeWeight="1" width="100%" />
                <HorizontalLine strokeColor="#d3d4d5" strokeWeight="1" width="100%" />
                <HorizontalLine strokeColor="#d3d4d5" strokeWeight="1" width="100%" />
                <HorizontalLine strokeColor="#d3d4d5" strokeWeight="1" width="100%" />
                <HorizontalLine strokeColor="#d3d4d5" strokeWeight="1" width="100%" />
              </GridLines>
              
              <!-- Line Chart -->
              <LineChart aspectRatio="4/1" width="100%">
                <ChartLine strokeColor="#8952e0" strokeWeight="2" />
              </LineChart>
            </Chart>
          </ChartArea>
        </MetricsCard>
        
        <!-- Data Cards Row -->
        <DataCardsRow display="flex" gap="28px">
          <!-- Top Countries Table Card -->
          <CountriesCard flex="1" padding="0 0 14px 0" display="flex" flexDirection="column" fillColor="#ffffff" strokeColor="#e7e7e8">
            <CardHeader padding="12px 14px" display="flex" alignItems="center">
              <CardTitle textStyle="text-sm/lineHeight-5/font-medium">Top countries</CardTitle>
            </CardHeader>
            
            <DataTable width="100%" display="flex" flexDirection="column">
              <!-- Table Header -->
              <TableHeader display="flex">
                <TableHeaderCell flex="2" padding="4px 16px" textStyle="text-xs/lineHeight-4/font-semibold">Country</TableHeaderCell>
                <TableHeaderCell flex="2" padding="4px 16px" textStyle="text-xs/lineHeight-4/font-semibold">Sales</TableHeaderCell>
                <TableHeaderCell flex="1" padding="4px 16px" textStyle="text-xs/lineHeight-4/font-semibold">Total</TableHeaderCell>
              </TableHeader>
              
              <!-- Table Rows -->
              <TableRow display="flex" alignItems="center" borderColor="#f1f1f2" borderWidth="1px">
                <TableCell flex="2" padding="4px 16px" textStyle="text-sm/lineHeight-5/font-normal">US</TableCell>
                <TableCell flex="2" padding="4px 16px" textStyle="text-sm/lineHeight-5/font-normal">30</TableCell>
                <TableCell flex="1" padding="4px 16px" textAlign="right" textStyle="text-sm/lineHeight-5/font-normal">€10,247</TableCell>
                <ProgressBar width="128px" height="4px" fillPercent="70" fillColor="#8952e0" backgroundColor="#f1f1f2" />
              </TableRow>
              
              <TableRow display="flex" alignItems="center" borderColor="#f1f1f2" borderWidth="1px">
                <TableCell flex="2" padding="4px 16px" textStyle="text-sm/lineHeight-5/font-normal">India</TableCell>
                <TableCell flex="2" padding="4px 16px" textStyle="text-sm/lineHeight-5/font-normal">210</TableCell>
                <TableCell flex="1" padding="4px 16px" textAlign="right" textStyle="text-sm/lineHeight-5/font-normal">€39,623</TableCell>
                <ProgressBar width="128px" height="4px" fillPercent="86" fillColor="#8952e0" backgroundColor="#f1f1f2" />
              </TableRow>
              
              <TableRow display="flex" alignItems="center" borderColor="#f1f1f2" borderWidth="1px">
                <TableCell flex="2" padding="4px 16px" textStyle="text-sm/lineHeight-5/font-normal">Brazil</TableCell>
                <TableCell flex="2" padding="4px 16px" textStyle="text-sm/lineHeight-5/font-normal">191</TableCell>
                <TableCell flex="1" padding="4px 16px" textAlign="right" textStyle="text-sm/lineHeight-5/font-normal">€12,413</TableCell>
                <ProgressBar width="128px" height="4px" fillPercent="70" fillColor="#8952e0" backgroundColor="#f1f1f2" />
              </TableRow>
              
              <TableRow display="flex" alignItems="center" borderColor="#f1f1f2" borderWidth="1px">
                <TableCell flex="2" padding="4px 16px" textStyle="text-sm/lineHeight-5/font-normal">Netherlands</TableCell>
                <TableCell flex="2" padding="4px 16px" textStyle="text-sm/lineHeight-5/font-normal">134</TableCell>
                <TableCell flex="1" padding="4px 16px" textAlign="right" textStyle="text-sm/lineHeight-5/font-normal">€19,996</TableCell>
                <ProgressBar width="128px" height="4px" fillPercent="60" fillColor="#8952e0" backgroundColor="#f1f1f2" />
              </TableRow>
              
              <TableRow display="flex" alignItems="center" borderColor="#f1f1f2" borderWidth="1px">
                <TableCell flex="2" padding="4px 16px" textStyle="text-sm/lineHeight-5/font-normal">US</TableCell>
                <TableCell flex="2" padding="4px 16px" textStyle="text-sm/lineHeight-5/font-normal">30</TableCell>
                <TableCell flex="1" padding="4px 16px" textAlign="right" textStyle="text-sm/lineHeight-5/font-normal">€10,247</TableCell>
                <ProgressBar width="128px" height="4px" fillPercent="35" fillColor="#8952e0" backgroundColor="#f1f1f2" />
              </TableRow>
              
              <TableRow display="flex" alignItems="center" borderColor="#f1f1f2" borderWidth="1px">
                <TableCell flex="2" padding="4px 16px" textStyle="text-sm/lineHeight-5/font-normal">United Kingdom</TableCell>
                <TableCell flex="2" padding="4px 16px" textStyle="text-sm/lineHeight-5/font-normal">47</TableCell>
                <TableCell flex="1" padding="4px 16px" textAlign="right" textStyle="text-sm/lineHeight-5/font-normal">€406</TableCell>
                <ProgressBar width="128px" height="4px" fillPercent="28" fillColor="#8952e0" backgroundColor="#f1f1f2" />
              </TableRow>
              
              <TableRow display="flex" alignItems="center">
                <TableCell flex="2" padding="4px 16px" textStyle="text-sm/lineHeight-5/font-normal">France</TableCell>
                <TableCell flex="2" padding="4px 16px" textStyle="text-sm/lineHeight-5/font-normal">43</TableCell>
                <TableCell flex="1" padding="4px 16px" textAlign="right" textStyle="text-sm/lineHeight-5/font-normal">€919</TableCell>
                <ProgressBar width="128px" height="4px" fillPercent="15" fillColor="#8952e0" backgroundColor="#f1f1f2" />
              </TableRow>
            </DataTable>
          </CountriesCard>
          
          <!-- Activity Feed Card -->
          <ActivityCard flex="1" padding="14px 20px" display="flex" flexDirection="column" gap="20px" fillColor="#ffffff" strokeColor="#e7e7e8">
            <CardHeader>
              <CardTitle textStyle="text-sm/lineHeight-5/font-medium">Activity</CardTitle>
            </CardHeader>
            
            <ActivityFeed display="flex" flexDirection="column">
              <ActivityItem display="flex" flexDirection="column">
                <ActivityHeader display="flex" gap="10px" alignItems="center">
                  <Avatar width="24px" height="24px" />
                  <ActivityContent display="flex" gap="3px" alignItems="center">
                    <UserName textStyle="text-sm/lineHeight-5/font-medium">User</UserName>
                    <ActivityDescription textStyle="text-sm/lineHeight-5/font-normal">signed up · Just now</ActivityDescription>
                  </ActivityContent>
                </ActivityHeader>
                <TimelineLine height="14px" width="1px" alignSelf="center" backgroundColor="#d3d4d5" />
              </ActivityItem>
              
              <ActivityItem display="flex" flexDirection="column">
                <ActivityHeader display="flex" gap="10px" alignItems="center">
                  <Avatar width="24px" height="24px" />
                  <ActivityContent display="flex" gap="3px" alignItems="center">
                    <UserName textStyle="text-sm/lineHeight-5/font-medium">User</UserName>
                    <ActivityDescription textStyle="text-sm/lineHeight-5/font-normal">signed up · Just now</ActivityDescription>
                  </ActivityContent>
                </ActivityHeader>
                <TimelineLine height="14px" width="1px" alignSelf="center" backgroundColor="#d3d4d5" />
              </ActivityItem>
              
              <ActivityItem display="flex" flexDirection="column">
                <ActivityHeader display="flex" gap="10px" alignItems="center">
                  <Avatar width="24px" height="24px" />
                  <ActivityContent display="flex" gap="3px" alignItems="center">
                    <UserName textStyle="text-sm/lineHeight-5/font-medium">User</UserName>
                    <ActivityDescription textStyle="text-sm/lineHeight-5/font-normal">upgraded to Pro · yesterday</ActivityDescription>
                  </ActivityContent>
                </ActivityHeader>
                <TimelineLine height="14px" width="1px" alignSelf="center" backgroundColor="#d3d4d5" />
              </ActivityItem>
              
              <ActivityItem display="flex" flexDirection="column">
                <ActivityHeader display="flex" gap="10px" alignItems="center">
                  <Avatar width="24px" height="24px" />
                  <ActivityContent display="flex" gap="3px" alignItems="center">
                    <UserName textStyle="text-sm/lineHeight-5/font-medium">User</UserName>
                    <ActivityDescription textStyle="text-sm/lineHeight-5/font-normal">upgraded to Pro · 2 days ago</ActivityDescription>
                  </ActivityContent>
                </ActivityHeader>
                <TimelineLine height="14px" width="1px" alignSelf="center" backgroundColor="#d3d4d5" />
              </ActivityItem>
              
              <ActivityItem display="flex" flexDirection="column">
                <ActivityHeader display="flex" gap="10px" alignItems="center">
                  <Avatar width="24px" height="24px" />
                  <ActivityContent display="flex" gap="3px" alignItems="center">
                    <UserName textStyle="text-sm/lineHeight-5/font-medium">User</UserName>
                    <ActivityDescription textStyle="text-sm/lineHeight-5/font-normal">upgraded to Pro · 3 days ago</ActivityDescription>
                  </ActivityContent>
                </ActivityHeader>
                <TimelineLine height="14px" width="1px" alignSelf="center" backgroundColor="#d3d4d5" />
              </ActivityItem>
              
              <ActivityItem display="flex" flexDirection="column">
                <ActivityHeader display="flex" gap="10px" alignItems="center">
                  <Avatar width="24px" height="24px" />
                  <ActivityContent display="flex" gap="3px" alignItems="center">
                    <UserName textStyle="text-sm/lineHeight-5/font-medium">User</UserName>
                    <ActivityDescription textStyle="text-sm/lineHeight-5/font-normal">signed up · 3 days ago</ActivityDescription>
                  </ActivityContent>
                </ActivityHeader>
              </ActivityItem>
            </ActivityFeed>
          </ActivityCard>
        </DataCardsRow>
      </DashboardContent>
    </MainContent>
  </AppLayout>
</SCREEN>